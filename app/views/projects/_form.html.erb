<%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :street %>
  <%= f.text_field :street %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.label :zip %>
  <%= f.number_field :zip %><br>
  <%= f.submit %>
<% end %>
